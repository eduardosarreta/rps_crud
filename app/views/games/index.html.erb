<h1>History of rounds</h1>

<table>
  <tr>
    <th> Computer move</th>
    <th>Player move</th>
    <th>Outome</th>
  </tr>
  <% Round.all.each do |round| %>

  <tr>
    <td><%= round.computer_move %></td>
    <td><%= round.player_move %></td>
    <td><%= round.outcome %></td>
  </tr>
  <% end %>
</table>
